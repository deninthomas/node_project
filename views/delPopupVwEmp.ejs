<div id="delete_employee">
    <div class="delete_content">
        <h3>Delete Employee</h3>
        <button id="close-btun" onclick="close_delete_employee()"><i class="fa-solid fa-xmark"></i></button>
    </div>
    <div class="delete_popup-content">
        <p>Are you sure you want to delete this employee?</p>
    </div>
    <div class="delete_btn">
        <button type="button" class="btn btn-secondary btn-main"
            onclick="close_delete_employee()">Cancel</button>
        <button type="button" class="btn btn-danger btn-delete" id="deleteConfirmation">Delete</button>
    </div>
</div>